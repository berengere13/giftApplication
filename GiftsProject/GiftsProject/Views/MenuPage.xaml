<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="GiftsProject.Views.MenuPage"
             Title="Menu">
    <StackLayout VerticalOptions="FillAndExpand">
      
        <Label Text="Mes envies">
            <Label.GestureRecognizers><TapGestureRecognizer Tapped="MesEnvies_Tapped"></TapGestureRecognizer></Label.GestureRecognizers>
            </Label>
        <Label Text="Leurs envies">
            <Label.GestureRecognizers><TapGestureRecognizer Tapped="LeursEnvies_Tapped"></TapGestureRecognizer></Label.GestureRecognizers>
            </Label>

    </StackLayout>
</ContentPage>